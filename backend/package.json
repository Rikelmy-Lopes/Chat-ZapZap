{
  "name": "chat-zapzap",
  "version": "1.0.0",
  "description": "",
  "main": "./build/server.js",
  "scripts": {
    "prestart": "rm -rf ./build",
    "start": "tsc --project tsconfig.json && node build/src/server.js",
    "dev": "tsnd src/server.ts",
    "build": "tsc --project tsconfig.json",
    "db:create": "npx sequelize-cli db:drop && npx sequelize-cli db:create",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:seed": "npx sequelize-cli db:seed:all",
    "db:reset": "npm run db:create && npm run db:migrate && npm run db:seed"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Rikelmy-Lopes/Chat-ZapZap.git"
  },
  "author": "Rikelmy-Lopes",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Rikelmy-Lopes/Chat-ZapZap/issues"
  },
  "homepage": "https://github.com/Rikelmy-Lopes/Chat-ZapZap#readme",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "crypto-js": "^4.1.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^7.5.1",
    "mysql2": "^3.6.1",
    "sequelize": "^6.33.0",
    "socket.io": "^4.7.2",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/crypto-js": "^4.1.2",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/sequelize": "^4.28.15",
    "@types/uuid": "^9.0.3",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "eslint": "^8.49.0",
    "eslint-config-standard-with-typescript": "^39.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-n": "^16.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2"
  }
}
